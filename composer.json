{
    "name": "ahmed/vega",
    "description": "Learn poo in php building a web framework",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "VegaCore\\":"vega_core"
        }
    },
    "authors": [
        {
            "name": "ahmed.b",
            "email": "baazizahmed@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages":true,
        "optimize-autoloader": true
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/var-dumper": "^7.2"
    },
    "require": {
        "php-di/php-di": "^7.0",
        "symfony/dotenv": "^7.2",
        "symfony/error-handler": "^7.2",
        "symfony/http-foundation": "^7.2"
    }
}
